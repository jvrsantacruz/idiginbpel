<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkMenu" id="proj_context_menu">
    <property name="visible">True</property>
    <property name="ubuntu_local">True</property>
    <child>
      <object class="GtkImageMenuItem" id="proj_context_menu_close">
        <property name="label" translatable="yes">Close</property>
        <property name="visible">True</property>
        <property name="image">proj_context_menu_close_img</property>
        <property name="use_stock">False</property>
        <property name="always_show_image">True</property>
      </object>
    </child>
    <child>
      <object class="GtkImageMenuItem" id="proj_context_menu_export">
        <property name="label" translatable="yes">Remove</property>
        <property name="visible">True</property>
        <property name="image">proj_context_menu_rm_img</property>
        <property name="use_stock">False</property>
        <property name="always_show_image">True</property>
      </object>
    </child>
    <child>
      <object class="GtkImageMenuItem" id="proj_context_menu_rm">
        <property name="label" translatable="yes">Export</property>
        <property name="visible">True</property>
        <property name="image">proj_context_menu_export_img</property>
        <property name="use_stock">False</property>
        <property name="always_show_image">True</property>
      </object>
    </child>
  </object>
  <object class="GtkImage" id="proj_context_menu_close_img">
    <property name="visible">True</property>
    <property name="xalign">0.49000000953674316</property>
    <property name="stock">gtk-close</property>
  </object>
  <object class="GtkImage" id="proj_context_menu_rm_img">
    <property name="visible">True</property>
    <property name="stock">gtk-delete</property>
  </object>
  <object class="GtkImage" id="proj_context_menu_export_img">
    <property name="visible">True</property>
    <property name="stock">gtk-redo</property>
  </object>
</interface>
